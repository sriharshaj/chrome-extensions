{
  "manifest_version": 2,
  "name": "Extensions Permissions",
  "description": "Displays all extensions required permissions",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["extensions.json"],
  "permissions": ["tabs"]
}
